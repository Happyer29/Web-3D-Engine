{
  "name": "webpacktests",
  "version": "1.0.0",
  "description": "",
  "main": "src/scripts/index.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/Happyer29/webpack-bootstrap-pug-template.git"
  },
  "scripts": {
    "start": "npm-run-all --parallel lint:watch build",
    "start:prod": "npm-run-all --parallel lint test build:prod",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "clean-build": "rimraf ./build && mkdir build",
    "clean": "npm-run-all clean-dist clean-build",
    "lint": "esw ./src/**/*.js",
    "lint:watch": "npm run lint -- --watch",
    "prebuild": "npm-run-all clean-dist",
    "build": "webpack --config ./config/webpack.dev.config.js",
    "build:watch": "webpack -w --config ./config/webpack.dev.config.js",
    "prebuild:prod": "npm-run-all clean-build",
    "build:prod": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.config.js",
    "postbuild:prod": "npm run server:prod",
    "test": "",
    "test:watch": ""
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@popperjs/core": "^2.9.3",
    "@types/mocha": "^9.0.0",
    "@types/webpack-env": "^1.16.3",
    "autoprefixer": "^10.3.2",
    "babel": "^6.23.0",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-watch": "^7.0.0",
    "etc": "^1.1.1",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.2.0",
    "mocha": "^9.1.2",
    "mocha-webpack": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^6.1.1",
    "style-loader": "^3.2.1",
    "ts-color-class": "^0.9.0",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.2",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0",
    "webpack-shell-plugin-next": "^2.2.2"
  },
